    "yarn": "yarn"
  },
  "author": "Meillet Robin <robin.meillet@epitech.eu>",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kMeillet/chocolatine.git"
  },
  "bugs": {
    "url": "https://github.com/kMeillet/chocolatine/issues"
  },
  "homepage": "https://github.com/kMeillet/chocolatine/#readme",
  "jest": {
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.js$",
    "moduleFileExtensions": ["js"]
  },
  "devDependencies": {
    "awesome-typescript-loader": "^2.2.4",
    "clean-webpack-plugin": "^0.1.13",
    "jest": "^16.0.2",
    "progress-bar-webpack-plugin": "^1.9.0",
    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.5",
    "typescript": "^2.0.3"
  },
  "dependencies": {
    "ramda": "^0.22.1",
    "webpack": "^2.1.0-beta.25"
  },
  "engines": {
    "node": ">= 6",
    "npm": ">= 3"
  }
